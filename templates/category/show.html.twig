{% extends 'base.html.twig' %}



{% block body %}

    {% for program in programsByCategory %}
        <br>
            <h2>{{ loop.index }} / {{ program.title }} </h2>
            <a href="{{ path("program_show", { id: program.id }) }}">Voir la fiche</a> </br>
            <p>{{ program.synopsis }}</p>
        </div>
    {% else %}
        Aucune série trouvée.
    {% endfor %}
{% endblock %}